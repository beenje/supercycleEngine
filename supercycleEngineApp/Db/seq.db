record(aSub, "$(P)$(R=)Seq0-Tst-aSubBuf_") {
    field(DESC, "Timestamp Array Buffer")
    field(SNAM, "ioASubBuf")
    field(SCAN, "Passive")

    field(FTA, "ULONG")
    field(NOA, "2046")
    field(FTVA, "ULONG")
    field(NOVA, "2046")

    field(INPA, "$(P)$(R=)EngineCycle_.VALC")
    field(OUTA, "$(PG)SoftSeq0-Timestamp-SP CP")

    field(FLNK, "$(P)$(R=)Seq0-Evt-aSubBuf_")
}

