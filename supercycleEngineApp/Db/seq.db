record(fanout, "$(P)$(R=)Seq0-Ctrl-FOut_") {
    field(DESC, "Seq Selector Fanout")
    field(SELM, "All")
    #field(PINI, "RUNNING")
    field(LNK1, "$(PG)SoftSeq1-Disable-Cmd.PROC CP")
    field(LNK2, "$(PG)SoftSeq0-Commit-Cmd.PROC CP")
    field(LNK3, "$(PG)SoftSeq0-Enable-Cmd.PROC CP")
}

