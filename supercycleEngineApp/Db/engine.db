    field(FTVA, "ULONG")
    field(NOVA, "10")
    field(FTVB, "ULONG")
    field(NOVB, "10")
    field(FTVC, "ULONG")
    field(NOVC, "10")
    field(FTVD, "ULONG")
    field(NOVD, "10")
}



