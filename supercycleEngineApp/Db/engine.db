record(calcout, "$(P)$(R=)Sel-Addr_") {
    field(DESC, "Select Address")
    field(CALC, "(A<1)?A+1:0")
    field(SCAN, "Passive")
    field(INPA, "$(P)$(R=)Sel-Addr_")
    field(FLNK, "$(P)$(R=)Sel-Mask_")
}

record(calcout, "$(P)$(R=)Sel-Mask_") {
    field(DESC, "Select Address")
    field(CALC, "(A>0)?0xFF00:0xFF")
    field(SCAN, "Passive")
    field(INPA, "$(P)$(R=)Sel-Addr_")

    field(FLNK, "$(P)$(R=)Tst-DMux-aSub_")
}

#==========
record(aSub, "$(P)$(R=)Tst-DMux-aSub_") {
    field(DESC, "Timestamp Array")
    field(SNAM, ioASubDMux)
    field(SCAN, "Passive")
    #alias("$(P)$(R=)IdCycle")
    field(VAL, "0")
    field(FTA, "ULONG")
    field(NOA, "10")
    field(FTVA, "ULONG")
    field(NOVA, "10")
    field(FTVB, "ULONG")
    field(NOVB, "10")

    field(INPA, "$(P)$(R=)EngineCycle.VALC")
    field(INPB, "$(P)$(R=)Sel-Addr_.VAL")

    $(ENEVG=)field(OUTA, "$(PEVG)SoftSeq0-Timestamp-SP CP")
    $(ENEVG=)field(OUTB, "$(PEVG)SoftSeq1-Timestamp-SP CP")

    field(FLNK, "$(P)$(R=)Evt-DMux-aSub_")
}

record(aSub, "$(P)$(R=)Evt-DMux-aSub_") {
    field(DESC, "Event Array")
    field(SNAM, ioASubDMux)
    field(SCAN, "Passive")
    #alias("$(P)$(R=)IdCycle")
    field(VAL, "0")
    field(FTA, "ULONG")
    field(NOA, "10")
    field(FTVA, "ULONG")
    field(NOVA, "10")
    field(FTVB, "ULONG")
    field(NOVB, "10")

    field(INPA, "$(P)$(R=)EngineCycle.VALD")
    field(INPB, "$(P)$(R=)Sel-Addr_.VAL")

    $(ENEVG=)field(OUTA, "$(PEVG)SoftSeq0-EvtCode-SP CP")
    $(ENEVG=)field(OUTB, "$(PEVG)SoftSeq1-EvtCode-SP CP")

    field(FLNK, "$(P)$(R=)Seq-Ctrl-seq_")
}

