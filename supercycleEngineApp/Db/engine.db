record(calcout, "$(P)$(R=)Sel-Addr_") {
    field(DESC, "Select Address")
    field(CALC, "(A<1)?A+1:0")
    field(SCAN, "Passive")
    field(INPA, "$(P)$(R=)Sel-Addr_")
    field(OUT,  "$(P)$(R=)Sel-Buf-dFOut_ PP MS")
    #field(FLNK, "$(P)$(R=)EngineCycle NPP MS")
}

record(dfanout, "$(P)$(R=)Sel-Buf-dFOut_") {
    field(OMSL, "supervisory")
    field(SCAN, "Passive")
    #field(DOL,  "$(P)$(R=)DMux-Out0_ CP MS")
    #field(DOL,  "$(P)$(R=)EngineCycle.VALA[4] CP MS")
    #$(ENEVG=)field(OUTA, "$(P)$(R=)Not0_.INPA CP")
    $(ENEVG=)field(OUTA, "$(P)$(R=)Seq1-dFout_ PP MS")
    $(ENEVG=)field(OUTB, "$(P)$(R=)Tst-DMux-aSub_ PP NMS")
    $(ENEVG=)field(OUTC, "$(P)$(R=)Evt-DMux-aSub_ PP NMS")
}

record(calcout, "$(P)$(R=)Not0_") {
    field(DESC, "Not Gate")
