record(aSub, "$(P)$(R=)EngineCycle") {
    #field(INAM, initEngine)
    field(SNAM, ioEngine)
    field(SCAN, "Passive")
    #alias("$(P)$(R=)IdCycle")
    field(FTVA, "ULONG")
    field(NOVA, "10")
    field(FTVB, "ULONG")
    field(NOVB, "10")
    field(FTVC, "ULONG")
    field(NOVC, "10")
    field(FTVD, "ULONG")
    field(NOVD, "10")

    #$(ENEVG=)field(OUTB, "$(PEVG)dbus-send-u32 PP")
    #$(ENEVG=)field(OUTC, "$(PEVG)SoftSeq0-Timestamp-SP PP")
    #$(ENEVG=)field(OUTD, "$(PEVG)SoftSeq0-EvtCode-SP PP")

    field(FLNK, "$(P)$(R=)Trig-FOut_")
}

record(fanout, "$(P)$(R=)Trig-FOut_") {
    field(DESC, "Trigger Fanout")
    field(SELM, "All")
    field(PINI, "RUNNING")
    field(LNK1, "$(P)$(R=)Sel-Addr_")
    field(LNK2, "")
}

