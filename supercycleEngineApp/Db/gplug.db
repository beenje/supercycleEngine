    field(SCAN, "Passive")

    field(FTA, "ULONG")
    field(NOA, "2046")
    field(FTVA, "ULONG")
    field(NOVA, "2046")

    # Processing trigger
    field(FTU, "ULONG")
    field(NOU, "1")
    #field(INPU, "$(P)$(R=)COffset-Seq_.LNK1 CA")

    field(INPA, "$(P)$(R=)EngineCycle_.VALB")
    field(OUTA, "$(PG)dbus-send-u32 CP")

    field(FLNK, "$(P)$(R=)CEnd-lOut_")
    #field(TPRO, "1")
}

