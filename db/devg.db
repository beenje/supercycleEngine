record(longout, "$(P)$(R=)SendEvt-SP") {
    field( DESC, "EVG Software Event Code")
    #field( DTYP, "")
    #field( OUT,  "")
    field( UDF,  "0")
    field( PINI, "YES") # so users don't complain about INVALID/UDF.  Safe as long as VAL=0. aka never autosave!
    field( VAL,  "0")
    field( HIHI, "256")
    field( LOLO, "-1")
    field( HHSV, "MAJOR")
    field( LLSV, "MAJOR")
    alias("$(P)$(R=)SoftEvt-EvtCode-SP")
    #field(TPRO, "1")
}

